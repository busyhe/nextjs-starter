{
  "$schema": "https://unpkg.com/@changesets/config@3.1.1/schema.json",
  "changelog": [
    "@changesets/changelog-github",
    {
      "repo": "busyhe/nextjs-starter"
    }
  ],
  "commit": false,
  "fixed": [
    [
      "@workspace/*",
      "web"
    ]
  ],
  "snapshot": {
    "prereleaseTemplate": "{tag}-{datetime}"
  },
  "privatePackages": {
    "version": true,
    "tag": true
  },
  "linked": [],
  "access": "restricted",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
  "ignore": []
}